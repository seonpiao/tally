@import '../common/base'
@import 'modules'
.keywords
  text-align:center
  padding:10px 0
  .item
    display:inline-block
    padding:10px 20px
    border:1px solid lighten($themeColor,20%)
    margin:10px
    border-radius(3px)
.custom
  text-align:center
  position:fixed
  bottom:0
  width:100%
  input
    width:70%
    font-size:16px
    padding:15px
    line-height:24px
    margin-bottom:10px
    border:1px solid rgba(black,.4)
    border-radius(3px)
    color:rgba(0,0,0,.8)
    outline:0