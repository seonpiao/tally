@import '../common/base'
@import 'modules'

.cover
  background:url(/image/bg.jpg)
  width:100%
  height:100%
  position:absolute
  -webkit-filter:blur(0px)
.panel
  $mainHeight = 120px
  height:$mainHeight
  position:absolute
  top:50%
  text-align:center
  margin-top:-($mainHeight/2)
  width:100%
  li
    height:($mainHeight/2)
    line-height:($mainHeight/2)
    text-align:center